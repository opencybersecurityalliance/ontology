{
    "openDxlApi": "0.1",
    "info": {
        "title": "Blacklist URL notifications",
        "version": "0.0.1",
        "description": "Blacklist URL notifications"
    },
    "events": {
        "/notification/blacklist/url": {
            "description": "Blacklist URL notification",
            "payload": {
                "properties": {
                    "$ref": "../openc2_schema.json#/definitions/x-target-url"
                },
                "required": [
                    "url"
                ],
                "example": {
                    "url": "http://example.com"
                }
            }
        }
    }
}